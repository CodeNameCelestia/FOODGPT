    <script>
        // bypass=true 
        var mobileLink = "";
        var desktopLink = "http://localhost:3000/foodbot?bypass=true";
        var phoneLink = document.querySelector('.phoneLink');
        
        function isMobile() {
            return window.innerWidth <= 600; // Adjust the width as needed
        }

        function updatePhoneLinkVisibility() {
            phoneLink.style.display = isMobile() ? 'block' : 'none';
            phoneLink.href = isMobile() ? mobileLink : desktopLink;

        }

        // Initial visibility check
        updatePhoneLinkVisibility();

        // Update visibility on window resize
        window.addEventListener('resize', updatePhoneLinkVisibility);

if (window.innerWidth <= 750) {
            // Select all elements with data-aos="fade-left" and replace with data-aos="fade-right"
            var fadeLeftElements = document.querySelectorAll('[data-aos="fade-left"]');
            fadeLeftElements.forEach(function(element) {
                element.setAttribute('data-aos', 'fade-right');
            });

            // Select all elements with data-aos="zoom-out-up" and replace with data-aos="fade-up"
            var zoomOutUpElements = document.querySelectorAll('[data-aos="zoom-out-up"]');
            zoomOutUpElements.forEach(function(element) {
                element.setAttribute('data-aos', 'fade-up');
            });

                

        }

document.getElementById('submitButton').addEventListener('click', async function () {
        const email = document.getElementById('email').value;
        const phone = document.getElementById('phone').value;
        const message = document.getElementById('message').value;
        try {
            const response = await fetch('/send-email', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify({ email, phone, message }),
            });
            if (response.ok) {
                console.log('Email sent successfully');
                // Optionally, you can display a success message or perform other actions here
            } else {
                console.error('Error sending email');
                // Optionally, you can display an error message or perform other actions here
            }
        } catch (error) {
            console.error(error.message);
            // Handle other errors here
        }
    });
    
document.addEventListener('DOMContentLoaded', function() {
        const tryMeNowButton = document.getElementById('tryMeNowButton');

        tryMeNowButton.addEventListener('click', function(event) {
            // Check if the user is authenticated (you need a way to determine this)
            // For example, assuming isAuthenticated is a global variable
            const isAuthenticated = <%= isAuthenticated %>; // Make sure to pass this value from the server

            if (!isAuthenticated) {
                // If the user is not authenticated, prevent the default link behavior
                event.preventDefault();
                // You can show a message to the user or perform other actions
                alert('Please log in to access this feature');
                window.location.href = '/login/';
            }
            // If the user is authenticated, the link will behave as normal
        });
    });
        

